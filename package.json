	"devDependencies": {
		"autoprefixer": "^9.4.7",
		"concat": "^1.0.3",
		"concurrently": "^4.1.0",
		"live-server": "^1.2.0",
		"node-sass": "^4.9.4",
		"postcss-cli": "^6.1.1"
	}
}
